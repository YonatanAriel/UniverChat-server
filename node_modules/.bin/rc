#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/yonat/OneDrive/שולחן העבודה/מסמכים/UniverChat server/node_modules/rc/node_modules:/mnt/c/Users/yonat/OneDrive/שולחן העבודה/מסמכים/UniverChat server/node_modules:/mnt/c/Users/yonat/OneDrive/שולחן העבודה/מסמכים/node_modules:/mnt/c/Users/yonat/OneDrive/שולחן העבודה/node_modules:/mnt/c/Users/yonat/OneDrive/node_modules:/mnt/c/Users/yonat/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/yonat/OneDrive/שולחן העבודה/מסמכים/UniverChat server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/yonat/OneDrive/שולחן העבודה/מסמכים/UniverChat server/node_modules/rc/node_modules:/mnt/c/Users/yonat/OneDrive/שולחן העבודה/מסמכים/UniverChat server/node_modules:/mnt/c/Users/yonat/OneDrive/שולחן העבודה/מסמכים/node_modules:/mnt/c/Users/yonat/OneDrive/שולחן העבודה/node_modules:/mnt/c/Users/yonat/OneDrive/node_modules:/mnt/c/Users/yonat/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/yonat/OneDrive/שולחן העבודה/מסמכים/UniverChat server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../rc/cli.js" "$@"
else
  exec node  "$basedir/../rc/cli.js" "$@"
fi
